SERVO ON
JOVRD 100

PHELPX=(+75.0, +1.5, +0.0, +0.0, +0.0, +0.0)
PHELPY=(-1.5, +75.0, +0.0, +0.0, +0.0, +0.0)
HOPEN 1

FOR I2=0 TO 2
FOR I1=0 TO 2

MOV P1+PHELPX*I1-PHELPY*I2, -50
MOV P1+PHELPX*I1-PHELPY*I2

DLY 0.1
HCLOSE 1
DLY 0.1

MOV P1+PHELPX*I1-PHELPY*I2, -50
MOV P2+PHELPX*I1+PHELPY*I2, -50
MOV P2+PHELPX*I1+PHELPY*I2

DLY 0.1
HOPEN 1
DLY 0.1

MOV P2+PHELPX*I1+PHELPY*I2, -50

NEXT I1
NEXT I2

SERVO OFF
END